{
  "name": "c0alk",
  "version": "1.0.0",
  "description": "Website cloaker with Nginx reverse proxy and management dashboard",
  "main": "server/index.js",
  "scripts": {
    "server": "nodemon server/index.js",
    "proxy": "nodemon server/proxy-server.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run proxy\" \"npm run client\"",
    "start": "node server/index.js",
    "start:proxy": "node server/proxy-server.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:proxy\"",
    "setup": "npm install && cd client && npm install",
    "migrate": "node server/migrate.js",
    "build": "cd client && npm run build"
  },
  "keywords": ["cloaker", "reverse-proxy", "nginx"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "axios": "^1.6.0",
    "geoip-lite": "^1.4.7",
    "useragent": "^2.3.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
